pipeline{
  agent any
  environment {
    ENV_URL = "pipeline.google.com"
  }
  stages{
    stage('One'){
      steps {
      ansiColor('xterm') {
          // some block
        sh '''echo -e "e[32m hello world[0m"
              echo hello Akram
              echo hello saria
              echo ENV_URL: ${ENV_URL}'''

      }
     }
     }
     stage('Two'){
           steps {
            sh ''' echo "stage Two"
             echo ENV_URL: ${ENV_URL}'''
           }
          }
    }
}
