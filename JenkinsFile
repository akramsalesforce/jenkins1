pipeline {
    agent any
    environment {
        ENV_URL = "pipeline.google.com"
        SSH_CRED = credentials('SSH')
    }

   stages {
          stage('One') {
              steps {
                  ansiColor('xterm') {
                      // some block
                      sh '''
                          echo -e "[32mhello world[0m"
                          echo hello Akram
                          echo hello saira
                          echo ENV_URL: ${ENV_URL}
                      '''
                  }
              }
          }

          stage('Two') {
              environment {
                 ENV_URL = "stage.google.com"
            }
             steps {
                sh '''
                    echo "stage Two"
                    echo ENV_URL: ${ENV_URL}
                    env
                '''
            }
        }
    }
}
