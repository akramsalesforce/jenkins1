pipeline{
  agent any


  environment {
    ENV_URL = "pipeline.google.com"
    SSH_CRED = credentials("SSH")
  }
  stages{
    stage('One'){
      steps {
      ansiColor('xterm') {
          // some block
        sh '''echo -e "e[32m hello world[0m"
              echo hello Akram
              echo hello saria
              echo ENV_URL: ${ENV_URL}'''

      }
     }
     }
     stage('Two'){
       environment {
         ENV_URL = "stage.google.com"
       }
           steps {
            sh '''
                echo "stage Two"
                echo ENV_URL: ${ENV_URL}
                echo 'env'
            '''
           }
          }
    }
}
